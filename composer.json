{
    "name": "testing",
    "description": "Respondent testing",
    "authors": [{
        "name": "Konstantin Saigushev",
        "email": "konstantin.saigushev@gmail.com"
    }],
    "autoload": {
        "psr-4": { "App\\": "src/" }
    },
    "autoload-dev": {
        "psr-4": { "App\\Tests\\": "tests/" }
    },
    "require": {
        "monolog/monolog": "1.0.*",
        "tmarois/filebase": "^1.0",
        "tightenco/collect": "^5.8"
    },
    "require-dev": {
        "phpunit/phpunit": "^8"
    },
    "scripts": {
        "start": "php -S localhost:8000",
        "test": "./vendor/bin/phpunit --bootstrap vendor/autoload.php tests/"
    }
}
